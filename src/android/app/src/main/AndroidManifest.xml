<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.processwarp.android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher"
                 android:label="@string/app_name" android:supportsRtl="true"
                 android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".RouterService" android:process=":router" />
        <service android:name=".WorkerService0" android:process=":worker0" />
        <service android:name=".WorkerService1" android:process=":worker1" />
        <service android:name=".WorkerService2" android:process=":worker2" />
    </application>

</manifest>
